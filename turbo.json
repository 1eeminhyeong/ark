{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "typedocs": {
      "cache": false
    },
    "storydocs": {
      "cache": false
    },
    "panda": {
      "cache": false
    },
    "build": {
      "outputs": ["dist/**", "styled-system/**"],
      "dependsOn": ["^build"]
    },
    "lint": {
      "outputs": []
    },
    "test:ci": {
      "outputs": []
    },
    "typecheck": {
      "outputs": []
    }
  }
}
