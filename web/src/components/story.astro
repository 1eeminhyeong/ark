---
import { Code } from 'astro/components'
import { getEntry } from 'astro:content'

interface Props {
  id: string
}
const { id } = Astro.props
const { framework, component } = Astro.params
const slug = [framework, component].join('/')

const stories = await getEntry('stories', slug)
---

{stories && <Code code={stories.data[id]} lang="tsx" />}
